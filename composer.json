{
    "name": "gemvc/library",
    "description": "core library of gemvc",
    "type": "library",
    "require": {
        "firebase/php-jwt": "^6.8",
        "phpmailer/phpmailer": "^6.8",
        "symfony/dotenv": "^7.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gemvc\\": "src/",
            "Gemvc\\Database\\": "src/database/",
            "Gemvc\\Database\\Query\\": "src/database/query/",
            "Gemvc\\Email\\": "src/email/",
            "Gemvc\\Helper\\": "src/helper/",
            "Gemvc\\Http\\": "src/http/",
            "Gemvc\\Core\\": "src/core/",
            "Gemvc\\Traits\\": "src/traits/",
            "Gemvc\\Traits\\Table\\": "src/traits/table/",
            "Gemvc\\Traits\\Model\\": "src/traits/model/",
            "Gemvc\\Traits\\Controller\\": "src/traits/controller/",
            "Gemvc\\CLI\\": "src/CLI/"
        },
        "files": [
            "src/database/SqlEnumCondition.php"
        ]
    },
    "authors": [
        {
            "name": "Ali Khorsandfard",
            "email": "ali.khorsandfard@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpstan/phpstan": "^2.1"
    }
}
